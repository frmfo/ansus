<%= turbo_stream.append "weighings" do %>
   <%= render partial: "weighings/weighing_list"%>
 <% end %>
<% if @pagy.next.present? %>
  <%= turbo_stream.replace "pagination" do %>
    <%= turbo_frame_tag "pagination", src: weighings_path(page: @pagy.next, format: :turbo_stream), loading: :lazy %>
  <% end %>
<% end %>